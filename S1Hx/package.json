{
  "name": "@slayone/extension",
  "displayName": "Slay One Hacking Extension | S1Hx",
  "version": "0.0.0",
  "description": "A basic Plasmo extension.",
  "author": {
    "name": "gemsvid√∏",
    "email": "gemsvido@gmail.com"
  },
  "scripts": {
    "dev": "plasmo dev",
    "build": "plasmo build",
    "package": "plasmo package"
  },
  "dependencies": {
    "plasmo": "0.60.2",
    "react": "18.2.0",
    "react-dom": "18.2.0"
  },
  "devDependencies": {
    "@plasmohq/prettier-plugin-sort-imports": "3.6.0",
    "@types/chrome": "0.0.202",
    "@types/node": "18.11.9",
    "@types/react": "18.0.25",
    "@types/react-dom": "18.0.9",
    "prettier": "2.7.1",
    "typescript": "4.9.3"
  },
  "manifest": {
    "host_permissions": [
      "https://*/*"
    ],
    "declarative_net_request": {
      "rule_resources": [
        {
          "id": "ruleset_1",
          "enabled": true,
          "path": "~rulesets/rules.json"
        }
      ]
    }
  }
}
